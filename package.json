{
	"name": "cwgame-api",
	"version": "0.0.1",
	"description": "cwgame API with NestJS",
	"author": "Aidan <idev.tech10@gmail.com>",
	"private": true,
	"license": "MIT",
	"scripts": {
		"build:prod": "rm -rf src/metadata.ts && rm -rf dist && cross-env NODE_ENV=production pnpm install && npx prisma migrate deploy && cross-env NODE_ENV=production nest build",
		"build:dev": "rm -rf src/metadata.ts && rm -rf dist && cross-env NODE_ENV=development pnpm install && npx prisma migrate deploy && cross-env NODE_ENV=development nest build",
		"start:dev": "nest start --watch",
		"start:staging": "NODE_ENV=staging nest start --watch",
		"start:prod": "NODE_ENV=production node dist/main.js",
		"start:debug": "nest start --debug 4400 --watch",
		"migrations": "ts-node ./src/infrastructure/database/runMigrations.ts",
		"migrations:prod": "NODE_ENV=production node dist/infrastructure/database/runMigrations.js",
		"migrations:dev": "NODE_ENV=development node dist/infrastructure/database/runMigrations.js",
		"seed:dev": "NODE_ENV=development npx kysely seed:run",
		"seed:prod": "NODE_ENV=production npx kysely seed:run",
		"db:seed": "ts-node prisma/seed.ts",
		"db:seed:prod": "NODE_ENV=production ts-node prisma/seed.ts",
		"new": "hygen new",
		"watch:dev": "ts-node-dev src/main.ts",
		"debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint --fix . --ext .ts",
		"test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
		"test:watch": "cross-env NODE_ENV=test jest --watch --detectOpenHandles --forceExit",
		"test:cov": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles --forceExit",
		"test:debug": "cross-env NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand --detectOpenHandles --forceExit",
		"test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
		"docs:dev": "vuepress dev -p 7070",
		"docs:build": "DEPLOY_ENV=gh-pages vuepress build",
		"docs:deploy": "pnpm docs:build && gh-pages -d .vuepress/dist",
		"prepare": "husky || true",
		"release": "release-it",
		"lint-staged": "lint-staged",
		"pm2:deploy:prod": "NODE_ENV=production pm2 deploy scripts/deploy/ecosystem.config.js production",
		"pm2:deploy:dev": "NODE_ENV=development pm2 deploy scripts/deploy/ecosystem.config.js development"
	},
